# Cricket Platform Environment Variables
# Copy this file to .env.local and fill in your values

# Node Environment
NODE_ENV=development

# Application URLs
NEXT_PUBLIC_WEB_URL=http://localhost:3000
NEXT_PUBLIC_DOCS_URL=http://localhost:3001

# API Configuration (if needed)
# NEXT_PUBLIC_API_URL=https://api.example.com
# API_KEY=your_api_key_here

# Database (if needed)
# DATABASE_URL=postgresql://user:password@localhost:5432/cricket_platform

# Authentication (if needed)
# NEXTAUTH_URL=http://localhost:3000
# NEXTAUTH_SECRET=your_nextauth_secret_here

# External Services (if needed)
# ANALYTICS_ID=your_analytics_id_here
# SENTRY_DSN=your_sentry_dsn_here

# Feature Flags (if needed)
# NEXT_PUBLIC_FEATURE_X_ENABLED=false
